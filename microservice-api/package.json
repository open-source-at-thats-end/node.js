{
  "name": "nodejs.microservice_api",
  "version": "0.0.1",
  "description": "API service for enterprise application",
  "author": "Thats End Team",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "build:app": "nest build app",
    "build:sharedapp": "nest build shared-app",
    "build:businessapp": "nest build business-app",
    "build:wsapp": "nest build ws-app",
    "start:dev:app": "NODE_ENV=development nest start --watch app",
    "start:dev:sharedapp": "NODE_ENV=development nest start --watch shared-app",
    "start:dev:businessapp": "NODE_ENV=development nest start --watch business-app",
    "start:dev:wsapp": "NODE_ENV=development nest start --watch ws-app",
    "start:debug:app": "NODE_ENV=test nest start --debug=60081 --watch app",
    "start:debug:sharedapp": "NODE_ENV=test nest start --debug=60082 --watch shared-app",
    "start:debug:businessapp": "NODE_ENV=test nest start --debug=60083 --watch business-app",
    "start:debug:wsapp": "NODE_ENV=test nest start --debug=60084 --watch ws-app",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:declares": "node tools/check-declare-fields.mjs",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/app/test/jest-e2e.json"
  },
  "dependencies": {
    "@apollo/client": "^4.0.8",
    "@apollo/gateway": "^2.11.3",
    "@apollo/server": "^5.0.0",
    "@apollo/subgraph": "^2.11.3",
    "@as-integrations/express5": "^1.1.2",
    "@golevelup/nestjs-graphql-request": "^0.2.5",
    "@nestjs/apollo": "^13.2.1",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.1.8",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.8",
    "@nestjs/graphql": "^13.2.0",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^11.1.8",
    "@nestjs/platform-socket.io": "^11.1.8",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/serve-static": "^5.0.4",
    "@nestjs/swagger": "^11.2.1",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.1.8",
    "@types/ua-parser-js": "^0.7.39",
    "argon2": "^0.44.0",
    "axios": "^1.13.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "date-fns": "^4.1.0",
    "express-session": "^1.18.2",
    "fs-extra": "^11.3.2",
    "graphql": "^16.11.0",
    "graphql-query-complexity": "^1.1.0",
    "graphql-scalars": "^1.25.0",
    "graphql-tools": "^9.0.20",
    "graphql-upload-ts": "^2.1.3",
    "hbs": "^4.2.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "joi": "^18.0.1",
    "jsonfile": "^6.2.0",
    "kill-port": "^2.0.1",
    "lodash": "^4.17.21",
    "mime-types": "^3.0.1",
    "multer": "^2.0.2",
    "mysql2": "^3.15.3",
    "nestjs-cls": "^6.0.1",
    "nestjs-pino": "^4.4.1",
    "nodemailer": "^7.0.10",
    "pino-http": "^10.5.0",
    "pino-pretty": "^13.1.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "sharp": "^0.34.4",
    "slugify": "^1.6.6",
    "socket.io": "^4.8.1",
    "typeorm": "^0.3.27",
    "ua-parser-js": "^2.0.6"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.8",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.5",
    "@types/fs-extra": "^11.0.4",
    "@types/handlebars": "^4.1.0",
    "@types/hbs": "^4.0.5",
    "@types/jest": "^30.0.0",
    "@types/kill-port": "^2.0.3",
    "@types/lodash": "^4.17.20",
    "@types/mime-types": "^3.0.1",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.9.1",
    "@types/nodemailer": "^7.0.3",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-loader": "^9.5.4",
    "ts-morph": "^25.0.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/libs/",
      "<rootDir>/apps/"
    ],
    "moduleNameMapper": {
      "^@libs/library-app(|/.*)$": "<rootDir>/libs/library-app/src/$1",
      "^@libs/dynamic-app(|/.*)$": "<rootDir>/libs/dynamic-app/src/$1"
    }
  }
}
