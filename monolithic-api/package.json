{
  "name": "raw-project",
  "private": true,
  "version": "1.0.0",
  "description": "Thats End Raw Project",
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "NODE_ENV=test nest start --debug=60081 --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/jest/bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "addnewenvvar": "ts-node scripts/add.new.env.var.ts",
    "ai:watch": "PYTHON_BIN=ai-agent/py/venv.sh node ai-agent/watch.js",
    "ai:sync": "ai-agent/py/venv.sh sync",
    "ai:ask": "ai-agent/py/venv.sh ask",
    "ai:last-change": "ai-agent/py/venv.sh last_change",
    "ai:new-day": "node -e \"const fs=require('fs'),p='ai-agent/sessions/'+new Date().toISOString().slice(0,10)+'.jsonl';fs.mkdirSync('ai-agent/sessions',{recursive:true});if(!fs.existsSync(p))fs.writeFileSync(p,'');console.log('Created',p)\""
  },
  "engines": {
    "npm": ">=10.0.0",
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@apollo/server": "^5.1.0",
    "@as-integrations/express5": "^1.1.2",
    "@golevelup/nestjs-graphql-request": "^0.2.5",
    "@nestjs/apollo": "^13.2.1",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/bullmq": "^11.0.3",
    "@nestjs/common": "^11.0.17",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/graphql": "^13.2.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/serve-static": "^5.0.3",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "argon2": "^0.44.0",
    "axios": "^1.11.0",
    "bullmq": "^5.58.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "date-fns": "^4.1.0",
    "graphql": "^16.12.0",
    "graphql-request": "^6.1.0",
    "hbs": "^4.2.0",
    "helmet": "^8.1.0",
    "joi": "^18.0.1",
    "jsonfile": "^6.2.0",
    "kill-port": "^2.0.1",
    "lodash": "^4.17.21",
    "mime-types": "^3.0.1",
    "mysql2": "^3.14.3",
    "nestjs-cls": "^6.0.1",
    "nestjs-pino": "^4.4.0",
    "nodemailer": "^7.0.5",
    "pino-http": "^10.5.0",
    "pino-pretty": "^13.1.1",
    "puppeteer": "^24.16.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "sharp": "^0.34.3",
    "slugify": "^1.6.6",
    "typeorm": "^0.3.26",
    "ua-parser-js": "^2.0.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@playwright/test": "^1.55.0",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.8",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "chokidar": "^4.0.3",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0",
    "yaml": "^2.8.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@libs/dynamic-app(|/.*)$": "<rootDir>/libs/dynamic-app/src/$1",
      "^@libs/library-app(|/.*)$": "<rootDir>/libs/library-app/src/$1"
    }
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@nestjs/core",
      "@swc/core"
    ]
  }
}
